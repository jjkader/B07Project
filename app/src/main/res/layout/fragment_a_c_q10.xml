<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AnnualCarbon.ACQ10Fragment" >

    <Button
        android:id="@+id/Q10Next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.9" />

    <TextView
        android:id="@+id/Q10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="How often do you waste food or throw away uneaten leftovers?"
        android:textAppearance="@style/TextAppearance.AppCompat.Body2"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <Button
        android:id="@+id/Q10Never"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Never"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/Q10Rarely"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Q10"
        app:layout_constraintVertical_bias="0.2" />

    <Button
        android:id="@+id/Q10Occasionally"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Occasionally"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/Q10Frequently"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Q10Never"
        app:layout_constraintVertical_bias="0.2" />

    <Button
        android:id="@+id/Q10Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@id/Q10Next"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/Q10Next"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/Q10Frequently"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Frequently"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/Q10Occasionally"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/Q10Occasionally"
        app:layout_constraintTop_toTopOf="@id/Q10Occasionally"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Q10Rarely"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rarely"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/Q10Never"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/Q10Never"
        app:layout_constraintTop_toTopOf="@id/Q10Never" />

    <TextView
        android:id="@+id/Q10Response"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/Q10Never"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Q10" />
</androidx.constraintlayout.widget.ConstraintLayout>